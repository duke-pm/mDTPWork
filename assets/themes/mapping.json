{
  "strict": {
    "text-font-family": "Roboto-Regular",

    "text-heading-1-font-size": 36,
    "text-heading-1-font-weight": "bold",
    "text-heading-1-font-family": "$text-font-family",

    "text-heading-2-font-size": 32,
    "text-heading-2-font-weight": "bold",
    "text-heading-2-font-family": "$text-font-family",

    "text-heading-3-font-size": 30,
    "text-heading-3-font-weight": "bold",
    "text-heading-3-font-family": "$text-font-family",

    "text-heading-4-font-size": 26,
    "text-heading-4-font-weight": "bold",
    "text-heading-4-font-family": "$text-font-family",

    "text-heading-5-font-size": 22,
    "text-heading-5-font-weight": "bold",
    "text-heading-5-font-family": "$text-font-family",

    "text-heading-6-font-size": 18,
    "text-heading-6-font-weight": "bold",
    "text-heading-6-font-family": "$text-font-family",

    "text-subtitle-1-font-size": 15,
    "text-subtitle-1-font-weight": "bold",
    "text-subtitle-1-font-family": "$text-font-family",

    "text-subtitle-2-font-size": 13,
    "text-subtitle-2-font-weight": "bold",
    "text-subtitle-2-font-family": "$text-font-family",

    "text-paragraph-1-font-size": 15,
    "text-paragraph-1-font-weight": "400",
    "text-paragraph-1-font-family": "$text-font-family",

    "text-paragraph-2-font-size": 13,
    "text-paragraph-2-font-weight": "400",
    "text-paragraph-2-font-family": "$text-font-family",

    "text-caption-1-font-size": 12,
    "text-caption-1-font-weight": "400",
    "text-caption-1-font-family": "$text-font-family",

    "text-caption-2-font-size": 12,
    "text-caption-2-font-weight": "300",
    "text-caption-2-font-family": "$text-font-family",

    "text-label-font-size": 12,
    "text-label-font-weight": "bold",
    "text-label-font-family": "$text-font-family"
  },
  "components": {
    "CheckBox": {
      "meta": {},
      "appearances": {
        "default": {
          "mapping": {
            "textFontSize": "text-paragraph-1-font-size",
            "textFontWeight": "text-paragraph-1-font-weight",
            "textFontFamily": "text-paragraph-1-font-family"
          }
        }
      }
    },
    "Toggle": {
      "meta": {},
      "appearances": {
        "default": {
          "mapping": {
            "textFontSize": "text-label-font-size",
            "textFontWeight": "text-label-font-weight",
            "textFontFamily": "text-label-font-family"
          }
        }
      }
    },
    "List": {
      "meta": {},
      "appearances": {
        "default": {
            "mapping": {
                "backgroundColor": "background-basic-color-3"
            }
        }
      }
    },
    "Card": {
      "meta": {},
      "appearances": {
        "outline": {
          "mapping": {
            "bodyPaddingHorizontal": 16,
            "bodyPaddingVertical": 10,
            "headerPaddingHorizontal": 16,
            "headerPaddingVertical": 10,
            "footerPaddingHorizontal": 16,
            "footerPaddingVertical": 10
          }
        }
      }
    },
    "Input": {
      "meta": {},
      "appearances": {
        "default": {
          "variantGroups": {
            "status": {
              "basic": {
                "state": {
                  "disabled": {
                    "labelColor": "text-hint-color"
                  }
                }
              },
              "primary": {
                "state": {
                  "disabled": {
                    "labelColor": "text-hint-color"
                  }
                }
              },
              "success": {
                "state": {
                  "disabled": {
                    "labelColor": "text-hint-color"
                  }
                }
              },
              "info": {
                "state": {
                  "disabled": {
                    "labelColor": "text-hint-color"
                  }
                }
              },
              "warning": {
                "state": {
                  "disabled": {
                    "labelColor": "text-hint-color"
                  }
                }
              },
              "danger": {
                "state": {
                  "disabled": {
                    "labelColor": "text-hint-color"
                  }
                }
              },
              "control": {
                "state": {
                  "disabled": {
                    "labelColor": "text-hint-color"
                  }
                }
              }
            }
          }
        }
      }
    },
    "Datepicker": {
      "meta": {},
      "appearances": {
        "default": {
          "mapping": {
            "paddingHorizontal": 0
          },
          "variantGroups": {
            "status": {
              "basic": {
                "textColor": "text-basic-color"
              }
            }
          }
        }
      }
    },
    "Radio": {
      "meta": {},
      "appearances": {
        "default": {
          "mapping": {
            "textFontSize": "text-paragraph-1-font-size",
            "textFontWeight": "text-paragraph-1-font-weight",
            "textFontFamily": "text-paragraph-1-font-family"
          }
        }
      }
    },
    "MenuItem": {
      "meta": {},
      "appearances": {
        "default": {
          "mapping": {
            "titleFontSize": "text-paragraph-1-font-size",
            "titleFontWeight": "text-paragraph-1-font-weight",
            "titleFontFamily": "text-paragraph-1-font-family"
          }
        }
      }
    },
    "OverflowMenu": {
      "meta": {},
      "appearances": {
        "default": {
          "mapping": {
            "width": 220
          }
        }
      }
    }
  }
}